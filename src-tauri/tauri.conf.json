{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:9420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "yd_tools",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true,
        "setAlwaysOnTop": true
      }
    },
    "bundle": {
      "windows": {
        "nsis":{
          "languages":["SimpChinese","English"]
        }
      },
      "active": true,
      "targets": "all",
      "identifier": "com.winged",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "customization",
        "fullscreen": false,
        "resizable": true,
        "title": "yd_tool",
        "width": 1000,
        "height": 800,
        "minWidth": 768,
        "decorations": false
      }
    ]
  }
}
